{% block head %}
<title>OWL Eyewear - Cr√©ation de page</title>
<link rel="icon" type="image/x-icon" href="{{ asset('/build/images/favicon.5ee79b31.ico') }}"/>
{% block stylesheets %}{{ encore_entry_link_tags('app') }}{% endblock %}
{% endblock %}
{% block body %}
    <div class="addItem">
        <div class="container">
            {{ form_start(form) }}
            {{ form_row(form.title) }}
            {{ form_row(form.image) }}
            {{ form_row(form.price) }}
            {{ form_row(form.reference) }}
            {{ form_row(form.gender) }}
            {{ form_row(form.brand) }}
            {{ form_row(form.material) }}
            {{ form_row(form.color) }}
            {{ form_row(form.shape) }}
            {{ form_row(form.styles) }}
            {{ form_row(form.mountType) }}
            {{ form_end(form)}}
        </div>
    </div>
{% endblock %}